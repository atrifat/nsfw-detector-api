{
  "name": "nsfw-detector-api",
  "version": "1.6.1",
  "description": "A simple PoC (Proof of Concept) NSFW Detector API Server",
  "exports": null,
  "type": "module",
  "scripts": {
    "start": "node src/index.mjs",
    "dev": "nodemon --delay 2 -q src/index.mjs",
    "test": "NODE_OPTIONS='--experimental-vm-modules --experimental-import-meta-resolve' jest",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules --experimental-import-meta-resolve' jest --config jest.e2e.config.mjs",
    "format": "prettier --write \"{src,__tests__}/**/*.mjs\"",
    "lint": "eslint \"{src,__tests__}/**/*.mjs\"",
    "format-check": "prettier --check \"{src,__tests__}/**/*.mjs\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.34.0",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.13",
    "babel-jest": "^29.7.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "async-mutex": "^0.5.0",
    "await-to-js": "^3.0.0",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-bearer-token": "^3.0.0",
    "js-sha256": "^0.11.1",
    "lru-cache": "^11.2.1",
    "mime": "^4.0.7",
    "p-limit": "^6.2.0",
    "p-memoize": "^7.1.1",
    "sharp": "^0.34.3",
    "url-regex-safe": "^4.0.0",
    "workerpool": "^9.3.3",
    "zod": "^3.25.76"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {
      "^.+\\.m?js$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    },
    "moduleNameMapper": {
      "mime": "<rootDir>/__mocks__/mime.mjs"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!mime|form-data|await-to-js|js-sha256).*/"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.mjs",
      "**/?(*.)+(spec|test).mjs"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/e2e/"
    ]
  }
}

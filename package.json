{
  "name": "nsfw-detector-api",
  "version": "1.6.1",
  "description": "A simple PoC (Proof of Concept) NSFW Detector API Server",
  "exports": null,
  "type": "module",
  "scripts": {
    "start": "node src/index.mjs",
    "dev": "nodemon --delay 2 -q src/index.mjs",
    "test": "NODE_OPTIONS='--experimental-vm-modules --experimental-import-meta-resolve' jest",
    "format": "prettier --write \"{src,__tests__}/**/*.mjs\"",
    "lint": "eslint \"{src,__tests__}/**/*.mjs\"",
    "format-check": "prettier --check \"{src,__tests__}/**/*.mjs\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.26.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "async-mutex": "^0.5.0",
    "await-to-js": "^3.0.0",
    "axios": "^1.8.4",
    "body-parser": "^2.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-bearer-token": "^3.0.0",
    "js-sha256": "^0.11.0",
    "lru-cache": "^11.1.0",
    "mime": "^4.0.7",
    "p-memoize": "^7.1.1",
    "sharp": "^0.34.1",
    "url-regex-safe": "^4.0.0",
    "workerpool": "^9.2.0",
    "zod": "^3.24.3"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {
      ".*\\.(mjs)$": ["babel-jest", { "rootMode": "upward" }],
      "^.+\\.js$": ["babel-jest", { "rootMode": "upward" }]
    },
    "moduleNameMapper": {
      "mime": "<rootDir>/__mocks__/mime.mjs"
    },
    "transformIgnorePatterns": ["/node_modules/(?!mime|form-data).*/"],
    "moduleFileExtensions": [
      "mjs",
      "js",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.mjs",
      "**/?(*.)+(spec|test).mjs"
    ]
  }
}
